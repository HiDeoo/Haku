{
  "name": "haku",
  "description": "Craft, consolidate and tackle your notebooks and to-do lists all in one place.",
  "homepage": "https://github.com/HiDeoo/haku",
  "version": "0.2.1",
  "author": "HiDeoo",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "pnpm build:sw && next dev",
    "db:migrate": "dotenv -c -- ts-node prisma/migrate.ts",
    "build": "next build && pnpm build:sw --prod",
    "build:sw": "ts-node scripts/serviceWorker",
    "build:analyze": "ANALYZE=true pnpm build",
    "build:screenshots": "ts-node scripts/screenshots",
    "start": "next start",
    "lint": "next lint -d . --max-warnings=0 && tsc --noEmit && tsc --noEmit -p src/sw/tsconfig.json",
    "test:api": "TEST_TYPE=api jest -i",
    "test:client": "TEST_TYPE=client jest -i",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "vercel-build": "prisma generate && pnpm db:migrate deploy && pnpm build"
  },
